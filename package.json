{
  "name": "ichi-api-updater",
  "private": true,
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "tsc",
    "build-local": "npm run clean && tsc && zip function.zip ./dist/*",
    "upload-dev": "aws lambda update-function-code --function-name ichi-api-updater-dev --zip-file fileb://dist/function.zip",
    "upload-staging": "aws lambda update-function-code --function-name ichi-api-updater-staging --zip-file fileb://dist/function.zip",
    "upload-prod": "aws lambda update-function-code --function-name ichi-api-updater-prod --zip-file fileb://dist/function.zip",
    "start": "ts-node ./src/index.ts",
    "dev": "ts-node ./src/dev.ts",
    "kovan": "ts-node ./src/kovan/dev.ts",
    "mumbai": "ts-node ./src/mumbai/dev.ts",
    "polygon": "ts-node ./src/polygon/dev.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.{ts,tsx}\""
  },
  "dependencies": {
    "@apollo/client": "^3.5.8",
    "@ichidao/ichi-sdk": "^0.0.34",
    "@thanpolas/univ3prices": "^3.0.2",
    "@uniswap/sdk": "^3.0.3",
    "axios": "^0.20.0",
    "coingecko-api": "^1.0.10",
    "coinmarketcap-api": "^3.1.1",
    "cross-fetch": "^3.1.5",
    "dotenv": "^16.0.2",
    "ethers": "^5.7.0",
    "graphql": "^16.3.0",
    "node-telegram-bot-api": "^0.59.0",
    "react": "^17.0.2",
    "ts-node-dev": "^2.0.0",
    "xirr": "^1.1.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.76",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.38",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "aws-lambda": "^1.0.6",
    "aws-sdk": "^2.896.0",
    "eslint": "^7.26.0",
    "eslint-plugin-import": "^2.22.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.2.4"
  }
}
